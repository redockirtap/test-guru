<% @test.questions.each do |question| %>
  <li>
    <p> <%= question.body %> </p>
    <%= link_to t('links.show'), admin_question_path(question) %> |
    <%= link_to t('links.edit'), edit_admin_question_path(question) %> |
    <%= link_to t('links.delete'), admin_question_path(question), method: :delete, data: {confirm: "Are you sure?"} %> 
  </li>
<% end %>